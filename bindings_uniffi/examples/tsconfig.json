{
    "compilerOptions": {
        "outDir": "dist",
        "sourceMap": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "node",
        "lib": ["ESNext"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}